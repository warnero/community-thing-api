{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./docs/meetup-api.jsdoc"],
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["./node_modules/jsdoc/plugins/markdown.js"],
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "./docs/template/",
        "encoding": "utf8",
        "destination": "./docs/build/",
        "tutorials": "./examples/"
    }
}